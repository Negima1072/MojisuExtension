{
	"name" : "MojisuCounter",
	"version" : "1.0.2",
	"permissions" : [
		"contextMenus",
		"http://*/*",
		"https://*/*"
	],
	"icons" : {
		"128" : "icon_128.png"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["js/content_script.js"]
	}],
	"background": {
		"scripts": ["js/background.js"]
	},
	"manifest_version": 2
}